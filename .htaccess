RewriteEngine on
RewriteCond %{HTTP_HOST} ^social.synans.com$ [NC,OR]
RewriteCond %{HTTP_HOST} ^www.social.synans.com$
RewriteCond %{REQUEST_URI} !Social/www/index.html/
RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L]

RewriteRule ^(.*) /Social/www/index.html [NC,L]